{
  "name": "audio_archive_api",
  "version": "0.0.0",
  "private": true,
  "description": "Backend Express API Template using Knex, PostgreSQL, Passport, and Typescript.",
  "keywords": [
    "example",
    "express",
    "passport",
    "knex",
    "pg",
    "template"
  ],
  "author": {
    "name": "Ben Chavez",
    "email": "ben.m.chavez@gmail.com",
    "url": "https://benjamin-chavez.com/"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjamin-chavez/node-pg-knex-passport-template"
  },
  "bugs": {
    "url": "https://github.com/benjamin-chavez/node-pg-knex-passport-template/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev1": "nodemon src/server.ts",
    "dev:clean": " (dropdb audio_archive_development || true) && createdb audio_archive_development && knex migrate:latest && knex seed:run && NODE_ENV=development pnpm dev",
    "lint": "eslint . --ext .ts",
    "test": "(dropdb audio_archive_test || true) && createdb audio_archive_test && jest",
    "test:watch": "(dropdb audio_archive_test || true) && createdb audio_archive_test && jest --watch",
    "db:migrate": "knex migrate:latest && node ./update-types",
    "start:server": "nodemon src/server.ts",
    "start:consumer": "nodemon src/jobs/consumer-worker.ts",
    "dev": "NODE_ENV=development concurrently \"pnpm run dev1\" \"pnpm run start:consumer\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
    "@aws-sdk/s3-request-presigner": "^3.427.0",
    "@shared": "workspace:*",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.1",
    "axios": "^1.6.0",
    "bullmq": "^4.12.8",
    "connect-session-knex": "^3.0.1",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-flash": "^0.0.2",
    "express-jwt": "^8.4.1",
    "express-list-endpoints": "^6.0.0",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "express-openid-connect": "^2.16.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "install": "^0.13.0",
    "ioredis": "^5.3.2",
    "jwks-rsa": "^3.0.1",
    "knex": "^2.5.1",
    "kysely-codegen": "^0.10.1",
    "morgan": "~1.10.0",
    "multer": "1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "stripe": "^14.3.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.425.0",
    "@types/amqplib": "^0.10.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-flash": "^0.0.2",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.5.9",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "concurrently": "^8.2.2",
    "husky": "^8.0.0",
    "jest": "^29.6.4",
    "knex-types": "^0.5.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  }
}
